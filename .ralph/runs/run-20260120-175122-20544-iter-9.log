+    opt.value = runId;
+    opt.textContent = runId;
+    ui.risRunSelect.appendChild(opt);
+  });
+  if (runList.length > 0) {
+    ui.risRunSelect.value = runList.includes(previous) ? previous : runList[0];
+  }
+  state.ris.runs = runList;
+}
